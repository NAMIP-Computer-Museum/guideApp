{
  "expo": {
    "name": "guideApp",
    "slug": "guideApp",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/old-computer.png",
    "splash": {
      "image": "./assets/old-computer.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*",
      "assets/**",
      "assets/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "be.namip.guideapp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/old-computer.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "be.namip.guideapp",
      "versionCode": 1
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
